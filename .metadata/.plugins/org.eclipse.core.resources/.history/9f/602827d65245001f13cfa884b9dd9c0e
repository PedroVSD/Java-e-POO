package p2_tropa;

public class Guerreiro extends Entidade{

	private int ataque;
	
	public int getAtaque() {
		return ataque;
	}

	public Guerreiro(String nome, int vida, int mana, int ataque, ArrayList<Item> itens) {
		super(nome, vida, mana, itens);
		this.ataque = ataque;
		calcularPoder();
	}

	@Override
	public int Poder() {
		// TODO Auto-generated method stub
		return 0;
	}
}
