package p2_tropa;

public class Entidade implements Lutavel{
	private int ataque;
	private int defesa;
	private int vida;
	private int mana;
	
	@Override
	public int calculaAtaque() {
		
	}
	public int getAtaque() {
		return ataque;
	}
	public void setAtaque(int ataque) {
		this.ataque = ataque;
	}
	public int getDefesa() {
		return defesa;
	}
	public void setDefesa(int defesa) {
		this.defesa = defesa;
	}
	public int getVida() {
		return vida;
	}
	public void setVida(int vida) {
		this.vida = vida;
	}
	public int getMana() {
		return mana;
	}
	public void setMana(int mana) {
		this.mana = mana;
	}
	@Override
	public int calculaDefesa() {
		
	}
	@Override
	public int calculaVida() {
		
	}
	@Override
	public int calculaManu() {
		
	}
}
