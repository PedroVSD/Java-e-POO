package fatura;

public class Fatura {
	private String nome;
	private String CPF;
	private Item itens[];
	
	public double obtemPreco() {
		
	}
	
	public void adicionaItem(int i, Item item) {
		
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getCPF() {
		return CPF;
	}

	public void setCPF(String cPF) {
		CPF = cPF;
