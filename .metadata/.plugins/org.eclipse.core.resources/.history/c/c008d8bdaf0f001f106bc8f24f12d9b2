package fatura;

public class Fatura {
	private String nome;
	private String CPF;
	private Item itens[];
	
	Item novo_item = new Item();
	
	public double obtemPreco() {
		double precoTotal = 0;
		for(int i = 0; i<=itens.length; i++) {
			precoTotal = novo_item.getQuantidade() * novo_item.getPrecoUnitario();
		}
		return precoTotal;
	}
	
	public void adicionaItem(int i, Item item) {
		
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getCPF() {
		return CPF;
	}

	public void setCPF(String cPF) {
		CPF = cPF;
	}

	public Item[] getItens() {
		return itens;
	}

	public void setItens(Item[] itens) {
		this.itens = itens;
	}
}
