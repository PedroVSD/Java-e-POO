package topico_04;

public class Fatura {
	private Item itens[];
	private String nome;
	private String CPF;

	public double obtemPreco() {
		double tot = 0;
		
		for(int i = 0; i < itens.length; i++) {
			tot += (itens[i].getQnt()*itens[i].getPrecoUnitario());
		}
		
		return tot;
	}
	
	public void adicionarItem(int i, Item novoItem) {
		this.itens[i] = novoItem;
	}
}
