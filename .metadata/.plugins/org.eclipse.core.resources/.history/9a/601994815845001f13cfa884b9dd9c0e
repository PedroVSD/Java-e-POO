package p2_tropa;

public class Item {
	protected String nome;
	protected int ataque;
	protected int defesa;
	protected int vida;
	protected int mana;
	
	public Item(String nome, int ataque, int defesa, int vida, int mana) {
		this.nome = nome;
		this.ataque = ataque;
		this.defesa = defesa;
		this.vida = vida;
		this.mana = mana;
	}
	
	
	
}
