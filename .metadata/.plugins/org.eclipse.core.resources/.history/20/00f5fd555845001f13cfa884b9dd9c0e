package p2_tropa;

import java.util.ArrayList;

public abstract class Entidade implements Lutavel{
	protected final String nome;
	protected int vida;
	protected int mana;
	protected int poder;
	protected ArrayList<Item> itens;
	
	public Entidade(String nome, int vida, int mana, ArrayList<Item> itens) {
		this.nome = nome;
		this.vida = vida;
		this.mana = mana;
		this.itens = itens;
	}
}
